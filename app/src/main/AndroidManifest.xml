<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seva_buddyv2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication.NoActionBar"
        android:hardwareAccelerated="false"
        android:largeHeap="true">
        <activity
            android:name="com.seva_buddyv2.NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.MyApplication.NoActionBar" >
        </activity>
        <activity android:name="com.seva_buddyv2.accType" />
        <activity android:name="com.seva_buddyv2.AccountCreation" />
        <activity android:name="com.seva_buddyv2.provider_AccountCreation" />
        <activity android:name="com.seva_buddyv2.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.seva_buddyv2.MainActivity" />
    </application>

</manifest>